{
  "require": {
    "barrelstrength/sprout-forms": "3.5.1",
    "barrelstrength/sprout-lists": "2.0.1",
    "bolden/htmlcache": "^1.0",
    "craftcms/cms": "3.3.13",
    "craftcms/redactor": "2.4.0",
    "ether/seo": "^3.6",
    "nystudio107/craft-minify": "^1.2",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
